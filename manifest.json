{
	"manifest_version": 3,
	"name": "JIRA Open",
	"version": "1.0",
	"description": "Open multiple JIRA links in tabs or as JIRA filter",
	"author": "theadisingh",
	"icons": {
		"16": "icons/JIRAOpen16.png",
		"48": "icons/JIRAOpen48.png",
		"128": "icons/JIRAOpen128.png"
	},
	"action": {
		"default_title": "JIRA Open",
		"default_icon": "icons/JIRAOpen128.png",
		"default_popup": "popup.html"
	},
	"permissions": ["tabs", "activeTab", "storage"]
}
